let a = "out";
let b = "b_out";

print(a);                       // out

if (true) {
    let a = "inner";

    print(a, b);                // inner b_out

    let b = "b_in";

    if (true) {
        let a = "inner2";
        print(a, b);            // inner2 b_in
    }

    let c = "c_in";

    print(a, b, c);             // inner b_in c_in
}

let c = "c_out";

print(a, c);                    // out c_out
